{
  "manifest_version": 3,
  "name": "Simple English Dictionary",
  "version": "0.0.1",
  "description": "Simple Side Panel Dictionary Plugin With Wiktionary",
  "icons": {
    "16": "images/dictionary-icon-16.png",
    "32": "images/dictionary-icon-32.png",
    "48": "images/dictionary-icon-48.png",
    "128": "images/dictionary-icon-128.png"
  },
  "permissions": [
    "sidePanel",
    "contextMenus",
    "storage"],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Click to open panel",
    "default_icon": {
      "16": "images/dictionary-icon-16.png",
      "32": "images/dictionary-icon-32.png",
      "48": "images/dictionary-icon-48.png",
      "128": "images/dictionary-icon-128.png"
    }
  },
  "side_panel": {
    "default_path": "side-panel/side-panel.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content_script.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}